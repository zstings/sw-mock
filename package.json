{
  "name": "sw-mock",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "author": "@zstings",
  "description": "一个基于 Service Worker 的轻量级、零侵入 API 模拟工具",
  "bin": {
    "sw-mock": "./dist/cli.mjs"
  },
  "files": [
    "dist",
    "public"
  ],
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "tsdown",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix --cache",
    "lint": "run-s lint:*",
    "format": "prettier --write --experimental-cli src/"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@prettier/plugin-oxc": "^0.0.5",
    "@tsconfig/node24": "^24.0.3",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.2",
    "eslint-plugin-oxlint": "~1.29.0",
    "jiti": "^2.6.1",
    "oxlint": "~1.29.0",
    "prettier": "3.6.2",
    "tsdown": "^0.18.3",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.50.1",
    "vite": "npm:rolldown-vite@latest"
  },
  "keywords": [
    "mock",
    "service-worker",
    "sw-mock"
  ],
  "homepage": "https://github.com/zstings/sw-mock#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zstings/sw-mock.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
